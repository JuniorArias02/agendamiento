{
  "build": {
    "env": {
      "NODE_OPTIONS": "--no-experimental-fetch"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
		"value": "*"
        }
      ]
    }
  ]
}
